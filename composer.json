{
    "name": "scandiweb/assessment",
    "description": "This package is meant to act as a backend that serves a front-end client to list products, add new one, and apply mass delete on products created.",
    "type": "project",
    "require": {
        "php": ">=7.4.0",
        "vlucas/phpdotenv": "5.4.x-dev",
        "nikic/fast-route": "2.0.x-dev",
        "illuminate/database": "8.x-dev",
        "respect/validation": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.21",
        "phpmd/phpmd" : "*",
        "squizlabs/php_codesniffer": "3.*",
        "guzzlehttp/guzzle": "7.5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Scandiweb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Scandiweb\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors tests/"
    },
